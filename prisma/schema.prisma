generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  birthDate DateTime
  cpf       String   @unique
  nickname  String
  gender    String
  email     String   @unique
  telephone String
  state     String
  country   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}